<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="controller.DoneTaskController"
      spacing="10" alignment="CENTER" prefWidth="400" prefHeight="400">

    <Button fx:id="backToList" id="backToList" onAction="#backToList" text="Back To Tasks"></Button>
    <Label fx:id="lblDoneTask" text="Done Tasks" style="-fx-font-size: 18px;"/>
    <ListView fx:id="doneList" prefWidth="300" prefHeight="200" />
</VBox>
